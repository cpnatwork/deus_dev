	<osgi:service id="attentionDaoOsgiService" ref="attentionDao" interface="deus.core.access.storage.api.attention.AttentionDao" />

	<osgi:service id="pubDaoOsgiService" ref="pubDao" interface="deus.core.access.storage.api.pub.api.PubDao" />
	
