	<osgi:service id="userDaoOsgiService" ref="userDao" interface="deus.core.access.storage.api.user.UserMetadataDoRep" />
	
	<osgi:service id="dossierDaoOsgiService" ref="dossierDao" interface="deus.core.access.storage.api.dossier.api.DossierDao" />
	
