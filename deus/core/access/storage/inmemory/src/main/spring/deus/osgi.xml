	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/osgi  
		http://www.springframework.org/schema/osgi/spring-osgi.xsd"
