	<osgi:service id="losDaoOsgiService" ref="losDao" interface="deus.core.access.storage.api.publication.LosDao" />
	
	<osgi:service id="losEntryDaoOsgiServce" ref="losEntryDao" interface="deus.core.access.storage.api.publication.LosEntryDao" />

	<osgi:service id="lopDaoOsgiService" ref="lopDao" interface="deus.core.access.storage.api.subscription.LopDao" />
	
