<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="userFactory" class="deus.core.UserFactory" />

	<bean id="gatekeeperTarget" class="deus.core.gatekeeper.impl.GatekeeperImpl" />

	<bean id="gatekeeper" class="deus.core.gatekeeper.impl.InitDestroyGatekeeperDecorator">
		<constructor-arg ref="gatekeeperTarget" />
	</bean>

	<bean id="remotingInitializerDestroyer" class="deus.remoting.initializerdestroyer.impl.DoNothingRemotingInitializerDestroyer" />

	<bean id="remoteSendingInitializerDestroyer" class="deus.remoting.initializerdestroyer.impl.DoNothingRemotingInitializerDestroyer" />
	
</beans>
