<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="xmppConfiguration" class="deus.nsi.xmpp.common.impl.XmppConfigurationImpl">
		<property name="xmppPropertyFullName" value="fullName"/>
	</bean>
	

	<bean id="xmppServerConnectionConfiguration" class="deus.nsi.xmpp.common.XmppServerConnectionConfiguration">
		<property name="compression" value="false" />
		<property name="saslAuthentication" value="false" />
		<property name="securityMode" value="disabled" />
	</bean>
	
</beans>
