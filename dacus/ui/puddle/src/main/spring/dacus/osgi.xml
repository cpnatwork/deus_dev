	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/osgi http://www.springframework.org/schema/osgi/spring-osgi.xsd">

	<osgi:reference id="gatekeeper" interface="deus.gatekeeper.Gatekeeper" />

	<osgi:reference id="userRegistry" interface="deus.core.soul.UserRegistry" />
	
	<osgi:reference id="userFactory" interface="deus.core.soul.UserFactory" />

</beans>
